{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning nodejs on mongoDb",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production && node server.js",
    "debug-win": "SET NODE_ENV=development && nodemon --inspect server.js",
    "debug-mac": "NODE_ENV=development && nodemon --inspect server.js",
    "populate-db": "node dev-data/data/import-dev-data --import",
    "populate-addresses": "node dev-data/data/import-addresses.js --import",
    "populate-users": "node dev-data/data/import-users.js --import",
    "drop-users": "node dev-data/data/import-users.js --delete",
    "drop-addresses": "node dev-data/data/import-addresses.js --delete",
    "populate-db-debug": "node --inspect dev-data/data/import-dev-data --import",
    "drop-db": "node dev-data/data/import-dev-data --delete",
    "drop-db-debug": "node --inspect dev-data/data/import-dev-data --delete",
    "debug-ndb": "ndb server.js",
    "debug-production": "SET NODE_ENV=production && nodemon --inspect server.js",
    "build-bundle": "webpack --config webpack.config.js",
    "build-bundle-prod": "webpack --mode production --config webpack.config.js"
  },
  "author": "Saulo",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.1.0",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.21.3",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "ndb": "^1.1.5",
    "nodemailer": "^6.4.2",
    "sharp": "^0.26.1",
    "slugify": "^1.3.6",
    "stripe": "^8.107.0",
    "validator": "^12.1.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "css-loader": "^4.2.0",
    "file-loader": "^6.0.0",
    "nodemon": "^1.19.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "^12"
  }
}
